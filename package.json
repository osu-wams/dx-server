{
  "name": "server",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "start": "pm2 kill && yarn run pm2:localhost",
    "saml": "pm2 kill && yarn run pm2:development",
    "pm2:localhost": "truncate -s 0 logs/localhost.log && cross-env NODE_ENV=localhost pm2 --no-daemon --env localhost start pm2.localhost.config.js",
    "pm2:development": "truncate -s 0 logs/development.log && cross-env NODE_ENV=development pm2 --no-daemon --env development start pm2.localhost.config.js",
    "monit": "pm2 monit",
    "lint": "eslint --ext .js,.ts 'src/**'",
    "lint:fix": "eslint --ext .js,.ts 'src/**' --fix",
    "test": "cross-env NODE_ENV=test USE_MOCKS=0 jest --no-coverage --detectOpenHandles --forceExit",
    "test:coverage": "NODE_ENV=test USE_MOCKS=0 jest --coverage --detectOpenHandles --forceExit"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/config": "^0.0.34",
    "@types/connect-redis": "^0.0.10",
    "@types/cookie-parser": "^1.4.1",
    "@types/express": "^4.17.0",
    "@types/express-session": "^1.15.13",
    "@types/jest": "^26.0.0",
    "@types/jsonwebtoken": "^8.3.7",
    "@types/nock": "^10.0.3",
    "@types/node": "^14.0.14",
    "@types/node-fetch": "^2.5.7",
    "@types/passport": "^1.0.0",
    "@types/passport-jwt": "^3.0.3",
    "@types/passport-local": "^1.0.33",
    "@types/passport-saml": "^1.1.2",
    "@types/redis": "^2.8.13",
    "@types/request-promise": "^4.1.44",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "codecov": "^3.7.1",
    "concurrently": "^5.0.2",
    "cross-env": "^5.2.0",
    "eslint": "^7.12.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.17.2",
    "eslint-utils": "^1.4.1",
    "husky": "^4.2.5",
    "jest": "^26.0.0",
    "msw": "^0.24.4",
    "nock": "^10.0.6",
    "nodemon": "^2.0.4",
    "pm2": "^4.5.1",
    "redis-mock": "^0.56.3",
    "supertest": "^4.0.2",
    "ts-jest": "^26.4.3",
    "ts-node": "^8.3.0",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "@osu-wams/lib": "^0.1.73",
    "aws-sdk": "^2.510.0",
    "body-parser": "^1.19.0",
    "config": "^3.2.2",
    "connect-redis": "^3.4.2",
    "cookie-parser": "^1.4.4",
    "date-fns": "^2.8.0",
    "date-fns-tz": "^1.0.12",
    "dynamodb-toolbox": "^0.3.1",
    "express": "^4.17.1",
