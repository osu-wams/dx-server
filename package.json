{
  "name": "server",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "start": "truncate -s 0 logs/localhost.log && cross-env NODE_ENV=localhost nodemon src/index.ts",
    "saml": "truncate -s 0 logs/development.log && cross-env NODE_ENV=development nodemon src/index.ts",
    "prebuild": "tsc --project . --noEmit",
    "lint": "eslint --ext .js,.ts 'src/**'",
    "lint:fix": "eslint --ext .js,.ts 'src/**' --fix",
    "test": "cross-env NODE_ENV=test USE_MOCKS=0 jest --no-coverage --detectOpenHandles --forceExit",
    "test:coverage": "NODE_ENV=test USE_MOCKS=0 jest --coverage --detectOpenHandles --forceExit"
  },
