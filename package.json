    "eslint": "^7.12.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.24.2",
    "eslint-utils": "^1.4.1",
    "husky": "^4.2.5",
    "jest": "^26.0.0",
    "msw": "^0.24.4",
    "nock": "^10.0.6",
    "nodemon": "^2.0.4",
