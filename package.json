    "@types/express-session": "^1.15.13",
    "@types/jest": "^26.0.0",
    "@types/jsonwebtoken": "^8.3.7",
    "@types/nock": "^10.0.3",
    "@types/node": "^14.0.14",
    "@types/node-fetch": "^2.5.7",
    "@types/passport": "^1.0.0",
    "@types/passport-jwt": "^3.0.3",
    "@types/passport-local": "^1.0.33",
    "@types/passport-saml": "^1.1.2",
